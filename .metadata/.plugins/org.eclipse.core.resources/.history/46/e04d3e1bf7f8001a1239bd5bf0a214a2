package Herança_Polimorfismo;

public class Preguica_Classe_Filha extends Animal_Classe_Mãe {

	private String subirArvores;
	
	public Preguica_Classe_Filha (String nome, String som, int idade, String subirArvores)
	
	{
		super (nome, som, idade);
		this.subirArvores=subirArvores;		
	}

	public String getSubirArvores() {
		return subirArvores;
	}

	public void setSubirArvores(String subirArvores) {
		this.subirArvores = subirArvores;
	}
	
	
	
}

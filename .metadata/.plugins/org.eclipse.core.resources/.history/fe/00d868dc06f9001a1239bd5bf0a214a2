package Herança_Polimorfismo;

public class Cavalo_Classe_Filha extends Animal_Classe_Mãe {

	private String correr;
	
	public Cavalo_Classe_Filha (String nome, String som, int idade, String correr)
	
	{
		super ();
		this.correr=correr;		
	}

	public String getCorrer() {
		return correr;
	}

	public void setCorrer(String correr) {
		this.correr = correr;
	}
	
	public String getSom()
	{
		return " Riiiih Riiiih Riiiih... " + super.getSom() + " Correr " + this.getCorrer();
	}
	
}

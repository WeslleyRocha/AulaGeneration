package Herança_Polimorfismo;

public class Animal_Classe_Mãe {

	private String nome, som;
	private int idade;
	
	/*public Animal_Classe_Mãe (String nome, String som, int idade)
	{	
		this.nome=nome;
		this.som=som;
		this.idade=idade;		
	}*/

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getSom() {
		return som;
	}

	public void setSom(String som) {
		this.som = som;
	}

	public int getIdade() {
		return idade;
	}

	public void setIdade(int idade) {
		this.idade = idade;
	}
	
}
